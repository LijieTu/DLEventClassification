                .hide();
            this.$progress = $('<div />')
                .addClass('progress')
                .css('position', 'relative')
                .appendTo(this.$el);
            this.$bar = $('<div />')
                .addClass('progress-bar')
                .css({
                    'position': 'absolute',
                    'bottom': 0, 'left': 0,
                })
                .appendTo(this.$progress);
            
            // Set defaults.
            this.update(); 
            this.updateDescription();
            
            this.listenTo(this.model, 'change:bar_style', function(model, value) {
                this.update_bar_style();
